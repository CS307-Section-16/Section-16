<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="game.section_16"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="game.section_16.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="game.section_16.InitializeScreen"
            android:label="@string/title_activity_initialize_screen" >
        </activity>
        <activity
            android:name="game.section_16.ContinueScreen"
            android:label="@string/title_activity_continue_screen" >
        </activity>
        <activity
            android:name="game.section_16.SettingScreen"
            android:label="@string/title_activity_setting_screen" >
        </activity>
        <activity
            android:name="game.section_16.HighScoresScreen"
            android:label="@string/title_activity_high_scores_screen" >
        </activity>
    </application>

</manifest>
